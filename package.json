	},
	"autoupdate": {
		"source": "git",
		"target": "git+https://github.com/Vladlukhanin/nativescript-quickblox-sdk.git",
		"basePath": "",
		"files": [
			"quickblox.min.js"
		]
	},
	"config": {
		"ghooks": {
			"pre-commit": "npm run lint"
		}
	},
	"scripts": {
		"lint": "jshint src --reporter=node_modules/jshint-stylish",
